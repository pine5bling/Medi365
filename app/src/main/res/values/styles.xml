    // BottomMainView

    <style name="BottomMainViewFrame">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dimen_56</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="BottomMainViewIcon">
        <item name="android:layout_width">@dimen/dimen_24</item>
        <item name="android:layout_height">@dimen/dimen_24</item>
    </style>

    <style name="BottomMainViewTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">@dimen/dimen_2</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/dimen_text_size_12</item>
        <item name="android:textColor">@color/neutral_7</item>
        <item name="android:fontFamily">@font/svn_medium</item>
    </style>

    //HomeMissionView

    <style name="HomeMissionViewFrame">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dimen_94</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="HomeMissionViewIcon">
        <item name="android:layout_width">@dimen/dimen_60</item>
        <item name="android:layout_height">@dimen/dimen_60</item>
        <item name="android:layout_marginTop">@dimen/dimen_6</item>
    </style>

    <style name="HomeMissionViewTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">@dimen/dimen_2</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/dimen_text_size_14</item>
        <item name="android:textColor">@color/green</item>
        <item name="android:fontFamily">@font/svn_regular</item>
    </style>

    //HeaderView
    <declare-styleable name="HeaderView">
        <attr name="hv_left_icon" format="reference" />
        <attr name="hv_show_left_icon" format="boolean" />
        <attr name="hv_center_text" format="string" />
        <attr name="hv_right_icon" format="reference" />
        <attr name="hv_show_right_icon" format="boolean" />
        <attr name="hv_show_divider" format="boolean" />
        <attr name="hv_show_notification_number" format="boolean" />
    </declare-styleable>

    //MissionView
    <declare-styleable name="MissionView">
        <attr name="mv_icon" format="reference" />
        <attr name="mv_text" format="string" />
        <attr name="mv_text_color" format="color" />
        <attr name="mv_background" format="reference" />
        <attr name="mv_show_text" format="boolean" />
    </declare-styleable>

    //TabView
    <declare-styleable name="TabView">
        <attr name="tv_text_first" format="string" />
        <attr name="tv_text_second" format="string" />
        <attr name="tv_text_third" format="string" />
        <attr name="tv_text_fourth" format="string" />
        <attr name="tv_show_center_second" format="boolean" />
    </declare-styleable>

    //OptionView
    <declare-styleable name="OptionView">
        <attr name="ov_label" format="string" />
    </declare-styleable>

